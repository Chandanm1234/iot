//c++
int led_1=3;
int led_2=4;
int led_3=5;
void setup(){
pinMode(led_1, OUTPUT);
pinMode(led_2, OUTPUT);
pinMode(led_3, OUTPUT);
Serial.begin(9600);
}
void loop(){
digitalWrite(led_1, HIGH);
delay(60000)//it will be on for 60 sec red light
digitalWrite(led_2, HIGH);
delay(10000)//it will be on for 10 sec red light
digitalWrite(led_3, HIGH);
delay(30000)//it will be on for 30 sec red light
}
