// C++ code
void setup()
{
  pinMode(2, OUTPUT);
  pinMode(3, OUTPUT);
  pinMode(4, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(6, OUTPUT);
  pinMode(7, OUTPUT);
  pinMode(8, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(10, OUTPUT);
}

void loop()
{
  int g=1;
  int i=0;
  do{
    while(g<=10){
      digitalWrite(g, HIGH);
      delay(1000);
      digitalWrite(g, LOW);
      delay(1000);
      g++;
    }
  
    i++;
  }while(i<=2);
}
